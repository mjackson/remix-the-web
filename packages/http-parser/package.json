{
  "name": "@mjackson/http-parser",
  "version": "0.1.0",
  "description": "High-performance HTTP/1.1 parser using llhttp WASM",
  "type": "module",
  "types": "./dist/http-parser.d.ts",
  "module": "./dist/http-parser.js",
  "main": "./dist/http-parser.cjs",
  "exports": {
    ".": {
      "types": "./dist/http-parser.d.ts",
      "import": "./dist/http-parser.js",
      "require": "./dist/http-parser.cjs",
      "default": "./dist/http-parser.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "llhttp/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:types": "tsc --project tsconfig.build.json",
    "build:esm": "esbuild src/http-parser.ts --outfile=dist/http-parser.js --format=esm --platform=node --sourcemap",
    "build:cjs": "esbuild src/http-parser.ts --outfile=dist/http-parser.cjs --format=cjs --platform=node --sourcemap",
    "build": "pnpm run clean && pnpm run build:types && pnpm run build:esm && pnpm run build:cjs && cp -r src/llhttp dist/ && cp dist/http-parser.d.ts dist/http-parser.d.cts",
    "clean": "rm -rf dist",
    "build:wasm": "node scripts/build-wasm.js --docker",
    "update:llhttp": "node scripts/update-llhttp.js",
    "test": "node --disable-warning=ExperimentalWarning --test src/*.test.ts",
    "test:benchmark": "node --disable-warning=ExperimentalWarning test/http-parser-benchmark.ts",
    "test:load": "node --disable-warning=ExperimentalWarning test/http-parser-load.ts",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "http",
    "parser",
    "request",
    "llhttp",
    "wasm",
    "performance",
    "undici"
  ],
  "author": "Michael Jackson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjackson/http-parser.git"
  },
  "bugs": {
    "url": "https://github.com/mjackson/http-parser/issues"
  },
  "homepage": "https://github.com/mjackson/http-parser#readme",
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.5",
    "typescript": "^5.0.0"
  }
}